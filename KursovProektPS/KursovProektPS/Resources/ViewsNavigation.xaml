<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views ="clr-namespace:KursovProektPS.Views"
                    xmlns:locals ="clr-namespace:KursovProektPS">
    <DataTemplate DataType="{x:Type locals:LoginVM}" x:Key="login">
        <views:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type locals:ResultsVM}" x:Key="results">
        <views:ResultsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type locals:TestSetupVM}" x:Key="setup">
        <views:TestSetupView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type locals:QuestionVM}" x:Key="question">
        <views:QuestionView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type locals:StatisticsVM}" x:Key="statistics">
        <views:StatisticsView/>
    </DataTemplate>
</ResourceDictionary>