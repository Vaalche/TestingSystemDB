<Application x:Class="KursovProektPS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:KursovProektPS"
             xmlns:Views="clr-namespace:KursovProektPS.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type local:MainWindowVM}">
            <local:MainWindow/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:LoginVM}">
            <Views:LoginView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:ResultsVM}">
            <Views:ResultsView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:TestSetupVM}">
            <Views:TestSetupView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type local:QuestionVM}">
            <Views:QuestionView/>
        </DataTemplate>
    </Application.Resources>	
</Application>
